<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ffe3e2e2" android:endColor="#ffe3e2e2" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffe3e2e2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ff60e72b" android:endColor="#ff84ed4b" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>