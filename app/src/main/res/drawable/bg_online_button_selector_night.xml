<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <solid android:color="@color/green_res_press_night" />
            <stroke android:width="1.0dip" android:color="@color/green_res_press_night" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <solid android:color="@color/green_res_night" />
            <stroke android:width="1.0dip" android:color="@color/green_res_night" />
        </shape>
    </item>
</selector>