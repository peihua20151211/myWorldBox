<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape>
                    <stroke android:width="1.5dip" android:color="#ffdedfe0" android:dashWidth="6.0dip" android:dashGap="3.0dip" />
                    <solid android:color="#1a959595" />
                </shape>
            </item>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/ic_photo_add" />
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape>
                    <stroke android:width="1.5dip" android:color="#ffdedfe0" android:dashWidth="6.0dip" android:dashGap="3.0dip" />
                    <solid android:color="@color/transparent" />
                </shape>
            </item>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/ic_photo_add" />
            </item>
        </layer-list>
    </item>
</selector>